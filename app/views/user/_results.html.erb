<div class="row"  id="user-results">

		
			<div class="panel-heading">
				Users
			</div>
			<div class="panel-body">
				<% if @users.empty? == false %>
				<%= will_paginate @users %>

				<% @users.each do |l| %>
					<div class="row">
						<div class="col-md-2">
							<p class="user_name"><%= l.name %></p>
							<p class="email"><%= l.email %></p>
							<p class="role"><%= l.role %></p>
						</div>
					</div>
				<% end %>
				<% end %>
			</div>
		
	
</div>